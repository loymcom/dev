<?xml version="1.0" encoding="utf-8" ?>
<data>
    <!-- Views -->
    <record id="teamm_view_tree" model="ir.ui.view">
        <field name="name">teamm</field>
        <field name="model">teamm</field>
        <field name="arch" type="xml">
            <tree>
                <field name="sequence" widget="handle" />
                <field name="name" />
                <field name="model_ids" widget="many2many_tags" />
                <field name="state" optional="show" />
                <field name="active" optional="hide" />
                <field name="date_format" />
            </tree>
        </field>
    </record>
    <record id="teamm_view_form" model="ir.ui.view">
        <field name="name">teamm form</field>
        <field name="model">teamm</field>
        <field name="arch" type="xml">
            <form>
                <header>
                    <button
                        name="action_import_api"
                        class="oe_highlight"
                        string="API Import"
                        type="object"
                    />
                    <button
                        name="action_import_csv"
                        class="oe_highlight"
                        string="CSV Import"
                        type="object"
                    />
                    <field name="state" widget="statusbar" />
                </header>
                <sheet>
                    <field name="active" invisible="1" />
                    <widget
                        name="web_ribbon"
                        title="Archived"
                        bg_color="bg-danger"
                        attrs="{'invisible': [('active', '=', True)]}"
                    />
                    <!-- <div class="oe_button_box" name="button_box">
                        <button
                            name="action_teamm"
                            type="object"
                            class="oe_stat_button"
                            icon="fa-globe"
                        >
                            <span>Teamm Action</span>
                        </button>
                    </div> -->
                    <div class="oe_title">
                        <h1>
                            <field name="name" placeholder="Record Name" />
                        </h1>
                    </div>
                    <group name="main">
                        <field name="model_ids">
                            <tree editable="bottom">
                                <field name="teamm_id" invisible="1" />
                                <field name="sequence" widget="handle" />
                                <field name="is_active" />
                                <field name="name" />
                            </tree>
                        </field>
                        <field name="url" />
                        <field name="param_ids">
                            <tree editable="bottom">
                                <field name="teamm_id" invisible="1" />
                                <field name="key" />
                                <field name="value" />
                            </tree>
                        </field>
                        <field name="date_format" />
                        <field name="csv" />
                    </group>
                </sheet>
            </form>
        </field>
    </record>
    <record id="teamm_view_search" model="ir.ui.view">
        <field name="name">teamm.view.search</field>
        <field name="model">teamm</field>
        <field name="arch" type="xml">
            <search>
                <field name="name" />
                <filter
                    name="name"
                    string="Team M"
                    domain="[('name', '=', 'Team M')]"
                />
            </search>
        </field>
    </record>
    <!-- Actions -->
    <record id="teamm_action" model="ir.actions.act_window">
        <field name="name">Team M</field>
        <field name="res_model">teamm</field>
        <field name="view_mode">tree,form</field>
        <!-- <field name="domain">[]</field> -->
        <!-- <field name="context">{}</field> -->
    </record>
    <menuitem
        id="teamm_menu"
        name="Team M"
        action="teamm_action"
        sequence="10"
    />
</data>
