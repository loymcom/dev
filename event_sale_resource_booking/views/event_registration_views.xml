<?xml version="1.0" encoding="utf-8" ?>
<odoo>
    <record id="event_registration_view_form" model="ir.ui.view">
        <field name="name">event.registration.view.form.inherit</field>
        <field name="model">event.registration</field>
        <field name="inherit_id" ref="event.view_event_registration_form" />
        <field name="priority" eval="20" />
        <field name="arch" type="xml">
            <group name="event" position="after">
                <group name="resource_booking" string="Resource Booking">
                    <field name="resource_booking_id" />
                    <field name="product_id" />
                    <field name="resource_booking_combination_id" />
                </group>
            </group>
        </field>
    </record>
</odoo>
