<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <menuitem
        id="product_template_menu"
        name="Products"
        action="sale.product_template_action"
        parent="resource_booking.resource_booking_type_configuration_menu"
        sequence="50"
    />

    <record id="product_template_form_view" model="ir.ui.view">
        <field name="name">product.template.view.tree</field>
        <field name="model">product.template</field>
        <field name="inherit_id" ref="product.product_template_form_view" />
        <field name="arch" type="xml">
            <notebook position="inside">
                <page name="date_range" string="Calendar">
                    <group name="date_range" string="Calendar">
                        <field name="date_range_ids" />
                    </group>
                </page>
            </notebook>
        </field>
    </record>

</odoo>
